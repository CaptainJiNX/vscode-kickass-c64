{
    "name": "kickassembler-c64",
    "displayName": "KickAssembler (C64)",
    "description": "Code like it's 1988!",
    "version": "0.0.1",
    "publisher": "CaptainJiNX",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ],
        "languages": [
            {
                "id": "kickassembler",
                "aliases": [
                    "KickAssembler (C64)",
                    "kickassembler"
                ],
                "extensions": [
                    ".asm",
                    ".inc",
                    ".s",
                    ".a",
                    ".lib"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "kickassembler",
                "scopeName": "source.assembly.kickassembler",
                "path": "./syntaxes/kickassembler.tmLanguage"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "vscode": "^1.1.21",
        "eslint": "^4.11.0"
    }
}
